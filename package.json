{
	"name": "fortuna-bot",
	"version": "2.0.0",
	"description": "Fortuna Bot release 2",
	"scripts": {
		"build": "tsc --build --verbose",
		"build:check": "tsc --noEmit --incremental false",
		"build:dev": "npx ts-node src/deploy-dev-commands.ts",
		"build:global": "npx ts-node src/deploy-global-commands.ts",
		"start": "npx ts-node src/index.ts",
		"test": "echo \"Error: no test specified\" && exit 1",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"prettier": "prettier --write ."
	},
	"dependencies": {
		"discord-api-types": "^0.37.53",
		"discord.js": "^14.12.1",
		"dotenv": "^16.3.1",
		"ts-node": "^10.9.1"
	},
	"devDependencies": {
		"@types/node": "^20.5.0",
		"eslint": "^8.47.0",
		"prettier": "^3.0.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.1.6"
	}
}
